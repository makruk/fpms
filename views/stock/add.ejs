<div class="container">
<p>以下の通り入荷します</p>
<form action="/stock/add/" method="post">
<div>

</div>
<table class="table striped bordered hovered">
<thead>
  <tr>
    <th>写真</th>
    <th>商品名</th>
    <th>カテゴリ</th>
    <th>値段</th>
    <th>在庫数</th>
    <th>入荷数</th>
  </tr>
</thead>
<tbody>
<% for(var i=0;i<stocks.length;i++){ %>
  <tr<% if(error["id"+stocks[i].id]){ %> class="bg-red fg-white"<% } %>>
    <th><img src="image/stock1.jpeg" alt="写真"></th>
    <th><%= stocks[i].name %></th>
    <th><%= stocks[i].category %></th>
    <th><%= stocks[i].price %>円</th>
    <th><%= stocks[i].number %>個</th>
    <th><%= stocks[i].param %><input type="hidden" name="id<%= stocks[i].id %>" value="<%= stocks[i].param %>">個</th>
  </tr>
<% } %>
</tbody>
</table>
		<input type="submit" value="確認" class = "button large info">
<a href="/stock/"><button type="button" class="large">キャンセル</button></a>
</form>
</div>