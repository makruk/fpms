<div class="container">
<p>以下の通り入荷します</p>
<form action="/stock/" method="post">
<table class="table striped bordered hovered">
<thead>
  <tr>
    <th>写真</th>
    <th>商品名</th>
    <th>カテゴリ</th>
    <th>値段</th>
    <th>在庫数</th>
    <th>入荷数</th>
  </tr>
</thead>
<tbody>
<%var cost=0;%>
  <% for(var i=0;i<number.length;i++){ %>
  <% if(number[i]!= 0 && number[i]!= ""){ %>
  <tr>
    <td><img src="<%= stocks[i].photo %>" alt="<%= stocks[i].name %>"></td>
    <td><%= stocks[i].name %></a></td>
    <td><%= stocks[i].category %></td>
    <td><%= stocks[i].price %>円</td>
    <td><%= stocks[i].number %>個</td>
    <td><%= number[i] %>個</td>
    <%cost += number[i]*stocks[i].price%>
  <% } %>
  </tr>
  <% } %>
  <tr>
    <td></td>
    <td></td>
    <td>計</td>
    <td><%= cost %>円</td>
    <td></td>
    <td></td>
  </tr>
</tbody>
</table>
		<input type="submit" value="確認" class = "button large info">
		<input type="button" value="キャンセル" onClick="location.href='/stock/'" class = "button large">
</form>
</div>